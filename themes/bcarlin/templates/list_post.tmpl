{% extends "base.tmpl" %}
{% block content %}
        <!--Body content-->
<section>
    <header>
        <h1>{{title}}</h1>
    </header>
    <ul>
    {% for post in posts %}
        <li>
            <a href="{{post.permalink()}}">{{post.title()}}</a>
            (<time datetime="{{post.formatted_date("%Y-%m-%d")}}">{{post.formatted_date("%d %b %Y")}}</time>)
        </li>
    {% endfor %}
    </ul>

</section>
        <!--End of body content-->
{% endblock %}
